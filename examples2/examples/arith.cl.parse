        method a2i : String s -> Int
            if
                binary =
                    call length
                        callee
                            id s
                    int 0
            then
                int 0
            else
                if
                    binary =
                        call substr
                            callee
                                id s
                            args
                                int 0
                                int 1
                        str "-"
                then
                    unary ~
                        call a2i_aux
                            args
                                call substr
                                    callee
                                        id s
                                    args
                                        int 1
                                        binary -
                                            call length
                                                callee
                                                    id s
                                            int 1
                else
                    if
                        binary =
                            call substr
                                callee
                                    id s
                                args
                                    int 0
                                    int 1
