        method a2i_aux : String s -> Int
            let
                vars
                    Int int
                        int 0
                block
                    let
                        vars
                            Int j
                                call length
                                    callee
                                        id s
                        let
                            vars
                                Int i
                                    int 0
                            while
                                binary <
                                    id i
                                    id j
                            loop
                                block
                                    assign int
                                        binary +
                                            binary *
                                                id int
                                                int 10
                                            call c2i
                                                args
                                                    call substr
                                                        callee
                                                            id s
                                                        args
                                                            id i
                                                            int 1
                                    assign i
                                        binary +
                                            id i
                                            int 1
                    id int
        method i2a : Int i -> String
            if
                binary =
                    id i
                    int 0
            then
                str "0"
            else
                if
                    binary <
                        int 0
                        id i
                then
                    call i2a_aux
                        args
                            id i
                else
                    call concat
                        callee
                            str "-"
                        args
                            call i2a_aux
                                args
                                    binary *
                                        id i
                                        unary ~
                                            int 1
        method i2a_aux : Int i -> String
            if
                binary =
                    id i
                    int 0
            then
                str ""
            else
                let
                    vars
                        Int next
                            binary /
                                id i
                                int 10
                    call concat
                        callee
                            call i2a_aux
                                args
                                    id next
                        args
                            call i2c
                                args
                                    binary -
                                        id i
                                        binary *
                                            id next
                                            int 10
